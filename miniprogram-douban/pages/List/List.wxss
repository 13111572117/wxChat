.log-list {
  display: flex;
  flex-direction: column;
  padding: 40rpx;
}
.log-item {
  margin: 10rpx;
}
.bangdan{
  padding: 20rpx 5%;
}
.bangdanCont{
  height: 200rpx;
  margin-bottom: 40rpx
}
.bangdanBanner image{
  width: 100%;
  height: 260rpx;
  margin-bottom: 15rpx;
  border-radius: 8rpx
}
.contImg{
  float: left;
  width: 30%;
  height: 120rpx
}
.contImg image{
  width: 100%;
  height: 200rpx;
  border-radius: 8rpx
}
.contImg_title {
  z-index: 2;
  position: absolute;
  width: 205rpx;
  height: 200rpx;
  border-radius: 8rpx;
  margin-top:-208rpx;
  background-color: rgba(94, 92, 92, 0.3)ï¼›
}
.contImg_title text{
  color: #eee;
  margin-top: 25rpx;
  display: block;
  text-align: center;
}
.contImg_title text:nth-of-type(2){
  margin-top: 0
}
.contImg_title text:nth-of-type(1) , .contImg_title text:nth-of-type(3){
  font-size: 30rpx;
  color: #969696;
}
.contImg_title text:nth-of-type(3){
  font-size: 40rpx
}
.contTitle{ 
  float: right;
  width: 64%;
  height: 160rpx;
  margin-top: 20rpx;
  border: 1rpx solid #969696;
  border-left: 0rpx;
  border-radius: 0 8rpx 8rpx 0;
  padding-left: 4%;
  line-height: 50rpx;
}
.contTitle navigator{
  font-size: 30rpx;
  color: #333333;
}
.contTitle navigator text{
  font-size: 24rpx;
  color: #ff4f4c;
}